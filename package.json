{
  "name": "koa-typescript-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=production && node -r ts-node/register -r tsconfig-paths/register index.ts",
    "debug": "export NODE_ENV=development && export DEBUG=koa-typescript-boilerplate:*,typeorm:* && ./node_modules/.bin/nodemon --exec node -r ts-node/register -r tsconfig-paths/register --inspect=9229 --watch '**/*' -e ts,js index.ts",
    "test": "./node_modules/.bin/jest --config test/jest.config.js test/"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "koa": "^2.13.1",
    "koa-body": "^4.1.0",
    "koa-compress": "^5.0.1",
    "koa-logger": "^3.2.0",
    "koa-mount": "^4.0.0",
    "koa-passport": "^4.1.4",
    "koa-router": "^10.0.0",
    "koa-session": "^6.1.0",
    "koa-static": "^5.0.0",
    "md5": "^2.2.1",
    "mysql2": "^2.1.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "stream-wormhole": "^1.1.0",
    "typeorm": "^0.2.16"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.22",
    "@types/koa": "^2.13.1",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-router": "^7.0.40",
    "@types/koa-session": "^5.10.3",
    "@types/koa-static": "^4.0.1",
    "@types/md5": "^2.3.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^4.2.3"
  }
}